            <geo:monumentDescription codeSpace="urn:ga-gov-au:monument-description-type">BRASS PLATE</geo:monumentDescription>
            <geo:monumentFoundation>Roof</geo:monumentFoundation>
            <geo:markerDescription>Brass platform supported by 3 poles</geo:markerDescription>
